@inproceedings{STOAViz,
author = {Dang, T. and Nguyen, L. and Karim, A. and Uddameri, V.},
title = {{STOAViz: Visualizing Saturated Thickness of Ogallala Aquifer}},
year = {2017},
isbn = {9783038680406},
publisher = {Eurographics Association},
address = {Goslar, DEU},
url = {https://doi.org/10.2312/envirvis.20171102},
doi = {10.2312/envirvis.20171102},
abstract = {In this paper, we introduce STOAViz, a visual analytics tool for analyzing the saturated thickness of the Ogallala aquifer. The saturated thicknesses are monitored by sensors integrated on wells distributed on a vast geographic area. Our analytics application also captures the trends and patterns (such as average/standard deviation over time, sudden increase/decrease of saturated thicknesses) of water on an individual well and a group of wells based on their geographic locations. To highlight the usefulness and effectiveness of STOAViz, we demonstrate it on the Southern High Plains Aquifer of Texas. The work was developed using feedback from experts at the water resource center at a university. Moreover, our technique can be applied on any geographic areas where wells and their measurements are available.},
booktitle = {Proceedings of the Workshop on Visualisation in Environmental Sciences},
pages = {37–41},
numpages = {5},
location = {Barcelona, Spain},
series = {EnvirVis '17},
}
