@article{PathwayCommons2011,
author = {Cerami, Ethan G. and Gross, Benjamin E. and Demir, Emek and Rodchenkov, Igor and Babur, Nadia and Schultz, Nikolaus and Bader, Gary D. and Sander, Chris}, 
title = {Pathway Commons, a web resource for biological pathway data},
volume = {39}, 
number = {suppl 1}, 
pages = {D685-D690}, 
year = {2011}, 
doi = {10.1093/nar/gkq1039}, 
abstract ={Pathway Commons (http://www.pathwaycommons.org) is a collection of publicly available pathway data from multiple organisms. Pathway Commons provides a web-based interface that enables biologists to browse and search a comprehensive collection of pathways from multiple sources represented in a common language, a download site that provides integrated bulk sets of pathway information in standard or convenient formats and a web service that software developers can use to conveniently query and access all data. Database providers can share their pathway data via a common repository. Pathways include biochemical reactions, complex assembly, transport and catalysis events and physical interactions involving proteins, DNA, RNA, small molecules and complexes. Pathway Commons aims to collect and integrate all public pathway data available in standard formats. Pathway Commons currently contains data from nine databases with over 1400 pathways.}, 
//url = {http://nar.oxfordjournals.org/content/39/suppl_1/D685.abstract}, 
eprint = {http://nar.oxfordjournals.org/content/39/suppl_1/D685.full.pdf+html}, 
journal = {Nucleic Acids Research} 
}

